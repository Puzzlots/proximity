{
  "formatVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_desc}",
  "authors": [
    "Zombii",
    "Lemon",
    "CrabKing",
    "Replet"
  ],
  "meta": {
    "icon": "proximity-audio:icons/icon.png"
  },
  "entrypoints": {
    "postInit": [
      "org.example.exmod.ExampleModPostinit"
    ],
    "client_init": [
      "org.example.exmod.ClientInitializer"
    ],
    "init": [
      "org.example.exmod.ExampleMod"
    ],
    "preInit": [
      "org.example.exmod.ExampleModPreinit"
    ]
  },
  "mixins": [
    "mixins/example.common.mixins.json",
    {
      "config": "mixins/example.client.mixins.json",
      "environment": "client"
    },
    {
      "config": "mixins/example.server.mixins.json",
      "environment": "server"
    }
  ],
  "depends": {
    "cosmic-reach": ">=0.4.5",
    "puzzle-loader": ">=3.0.2",
    "cosmic-api": ">=1.2.1"
  },
  "accessTransformers": [
    "example_mod.manipulator"
  ]
}
